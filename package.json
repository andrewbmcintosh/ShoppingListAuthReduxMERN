{
  "name": "mernshoppinglist",
  "version": "1.0.0",
  "description": "Shopping List built with the MERN stack",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewbmcintosh/authExercise.git"
  },
  "author": "Andrew B. McIntosh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrewbmcintosh/authExercise/issues"
  },
  "homepage": "https://github.com/andrewbmcintosh/authExercise#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^4.1.0",
    "config": "^3.0.1",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.4.20"
  },
  "devDependencies": {
    "nodemon": "^1.18.10"
  }
}
